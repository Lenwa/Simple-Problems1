def is_triangle(a,b,c):
	if a + b > c and a + c > b and b + c > a:
		print "Yes, this is a triangle."
	else: 
		print "No, this is not a triangle."
#is_triangle(10, 2, 2)

def call_triangle():
	a = int(raw_input("Input value for side a of the triangle.\n"))
	b = int(raw_input("Input value for side b of the triangle.\n"))
	c = int(raw_input("Input value for side c of the triangle.\n"))
	is_triangle(a,b,c)
call_triangle()
